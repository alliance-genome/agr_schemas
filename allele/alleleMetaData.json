{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Basic Allele Information",
  "description": "A file of Basic Allele Information objects.",
  "id": "alleleMetaData.json",
  "type": "object",
  "required": [ "metaData", "data" ],
  "properties": {
    "metaData": {
      "$ref": "../metaData.json#",
      "description": "meta data of the load file as defined in metaData.json."
    },
    "data": { 
      "type": "array",
      "description": "A list of allele objects.",
      "items": {
        "$ref" : "allele.json#"
      }
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "nonGeneSequenceFeatureMetaData",
  "description": "A file of non gene sequence feature objects.",
  "id": "nonGeneSequenceFeatureMetaData.json",
  "type": "object",
  "required": [ "metaData", "data" ],
  "properties": {
    "metaData": {
      "$ref": "../metaData.json#",
      "description": "meta data of the load file as defined in metaData.json."
    },
    "data": { 
      "type": "array",
      "minItems": 1,
      "description": "A list of nonGeneSequenceFeature objects.",
      "items": {
        "$ref" : "nonGeneSequenceFeature.json"
      }
    }
  }
}

{
"$schema": "http://json-schema.org/draft-04/schema#",
"title": "Microarray Sample Details",
"description": "An entry with microarray-specific metadata",
"id": "microarraySampleDetails.json#",
"type": "object",
"additionalProperties": false,
"properties": {
    "channelId": {
        "type": "string",
        "description": " for microarray experiments, channel ID"
    },
    "channelNum": {
        "type": "integer",
        "enum": [1,2],
        "description": "for micorarray data, which channel this sample is in, 1 or 2"
    }
}
}
